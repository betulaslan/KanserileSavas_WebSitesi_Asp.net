@model List<Hakkimizda>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}

<h1>Index</h1>
<!doctype html>
<html lang="zxx">
<body>
    <!-- about breadcrumb -->
    <section class="w3l-about-breadcrumb text-left">
        <div class="breadcrumb-bg breadcrumb-bg-about py-sm-5 py-4">
            <div class="container">
                <h2 class="title">Erken Tanı Hayat Kurtarır </h2>
                <ul class="breadcrumbs-custom-path mt-2">
                    <li><a href="/AnaSayfa/Index/">Ana Sayfa</a></li>
                    <li class="active"><span class="fa fa-arrow-right mx-2" aria-hidden="true"></span> Hakkımızda </li>
                </ul>
            </div>
        </div>
    </section>
    <!-- //about breadcrumb -->
    <!--  Work gallery section -->
                    @foreach (var item in Model)
                    {
                        <section class="grids-1 py-5">
                            <div class="grids py-lg-5 py-md-4">
                                <div class="container">
                                    <h3 class="hny-title mb-5">HAKKIMIZDA</h3>
                                    <div class="row">
                                        @item.Aciklama
                                        <br /> <br>
                                        <h5>İLETİŞİM</h5><br />
                                        <div><br>
                                            <br />
                                                   Telefon Numarası : @item.TelNo <br />
                                        Adres : @item.Adres <br />
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </section>
                    }

    <link rel="stylesheet" href="~/Traversal-Liberty/assets/css/style-liberty.css" />
    <script src="~/Traversal-Liberty/assets/js/bootstrap.min.js"></script>
</body>

</html>

