@model EntityLayer.Concrete.AnaSayfa
@{
    ViewData["Title"] = "Detay";
    Layout = "~/Views/Shared/_UILayout.cshtml";
}
<!doctype html>
<html lang="zxx">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>Kanser ile Savaş | Blog Single </title>
    <!-- google fonts -->
    <link href="//fonts.googleapis.com/css2?family=Montserrat:wght@300;400;600;700&display=swap" rel="stylesheet">
    <link href="//fonts.googleapis.com/css2?family=Lato:ital,wght@0,300;0,400;0,700;1,400&display=swap"
          rel="stylesheet">
    <!-- google fonts -->
    <!-- Template CSS -->
    <link rel="stylesheet" href="assets/css/style-liberty.css">
    <!-- Template CSS -->
</head>

<body>

    <!-- about breadcrumb -->
    <section class="w3l-about-breadcrumb text-left">
        <div class="breadcrumb-bg breadcrumb-bg-about py-sm-5 py-4">
            <div class="container">
                <h2 class="title">Erken Teşhis Hayat Kurtarır</h2>
                <ul class="breadcrumbs-custom-path mt-2">
                    <li><a href="/AnaSayfa/Index/">Ana Sayfa</a></li>
                </ul>
            </div>
        </div>
    </section>
    <!--/blog-->
    <div class="py-5 w3l-homeblock1 text-center">
        <div class="container mt-md-3">
            <h3 class="blog-desc-big text-center mb-4">////////////</h3>
            <div class="blog-post-align">
                <div class="blog-post-img">
                </div>
                <div class="blog-post-info">
                </div>
            </div>
        </div>
    </div>
    <section class="blog-post-main w3l-homeblock1">
        <!--/blog-post-->
        <div class="blog-content-inf pb-5">
            <div class="container pb-lg-4">
                <div class="single-post-image">
                    <div class="post-content">
                        <img src=@Model.Resim class="radius-image img-fluid mb-5" alt="" style="width:1100px; height:400px;" />
                    </div>
                </div>
                <div class="single-post-content">
                    <p class="alphabet mb-4"><span class="big-letter">K</span>@Model.Detay1</p>
                    <h3 class="blog-desc-big m-0 mb-4"></h3>

                    <div class="sing-post-thumb mb-5 row pt-3">
                        <div class="col-sm-6">
                            <a href="#url"><img src=@Model.Resim2 class="img-fluid radius-image" alt="" style="width:1100px; height:400px;"></a>
                        </div>

                    </div>
                    <h3 class="blog-desc-big m-0 mb-lg-4 mb-3"></h3>
                    <p class="mb-4">
                        @Model.Detay2
                    </p>

                </div>
            </div>
            <!--//blog-post-->
        </div>
    </section>
</body>

</html>

